FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     pimpleParticleFoam;

startFrom       latestTime;

startTime       1.9;

stopAt          endTime;

endTime         2;

deltaT          1e-07;

writeControl    adjustableRunTime;

writeInterval   0.01;

purgeWrite      0;

cycleWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  yes; 

maxCo           0.95;

maxDeltaT       5e-03;


